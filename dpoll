#!/bin/bash
[[ "$*" =~ ([0-9]{1,3}\.){3}[0-9]{1,3} ]] && { ip=${BASH_REMATCH[0]}; set -- "${@#$ip}"; }

server=$(dserver "$ip")
read -r y m d H M < <(date -d'1 hours ago' +'%Y %m %d %H %M')

case $server in
   
   Embedthis-Appweb*)

      dcp /video/$y$m$d/$H
   ;;

   *)

   echo -e "Server not supported $server"
   ;;
esac
